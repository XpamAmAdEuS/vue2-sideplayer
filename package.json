{
  "name": "vue2-sideplayer",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:esm & npm run build:umd &  wait && echo \"Build complete:\nesm: $(gzip -c dist/vue2sideplayer.esm.js | wc -c)b gzip\numd: $(gzip -c dist/vue2sideplayer.umd.js | wc -c)b gzip\nmin: $(gzip -c dist/vue2sideplayer.min.js | wc -c)b gzip\"",
    "build:esm": "rollup --config build/rollup.config.js --format es --file dist/vue2sideplayer.esm.js",
    "build:size": "gzip -c dist/vue2sideplayer.esm.js | wc -c",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue2sideplayer.umd.js"
  },
  "main": "dist/vue2sideplayer.umd.js",
  "module": "dist/vue2sideplayer.esm.js",
  "browser": {
    "./sfc": "src/index.ts"
  },
  "unpkg": "dist/vue2sideplayer.min.js",
  "dependencies": {
    "mitt": "^2.1.0",
    "wavesurfer.js": "^4.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/wavesurfer.js": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vue/composition-api": "^1.0.0-rc.6",
    "node-sass": "^5.0.0",
    "rollup": "^2.45.2",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-internal": "^1.0.4",
    "rollup-plugin-multi-input": "^1.2.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^11.0.1",
    "typescript": "~4.2.4",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "bugs": {
    "url": "https://github.com/XpamAmAdEuS/vue2-sideplayer/issues"
  },
  "contributors": [
    "Miljenko Halapir <miljenkohalapir@gmail.com>"
  ],
  "keywords": [
    "vue",
    "wavesurfer"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/XpamAmAdEuS/vue2-sideplayer.git"
  }
}
